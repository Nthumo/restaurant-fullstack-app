{
    "builds": [
      {
        "src": "resto/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "resto/dist",
          "buildCommand": "npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "status": 200
      }
    ]
  }
  