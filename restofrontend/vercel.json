{
    "builds": [
      {
        "src": "restofrontend/resto/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "restofrontend/resto/dist",
          "buildCommand": "npm run build"
        }
      }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html "
        }
    ]
  }
  